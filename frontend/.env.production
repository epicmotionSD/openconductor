# OpenConductor AI Terminal - Production Environment Configuration

# Application Settings
VITE_APP_NAME=OpenConductor AI Terminal
VITE_APP_VERSION=1.0.0
VITE_ENVIRONMENT=production

# API Configuration
VITE_API_BASE_URL=https://api.openconductor.ai
VITE_WEBSOCKET_URL=wss://api.openconductor.ai/ws

# Trinity AI Configuration
VITE_TRINITY_VERSION=1.0.0
VITE_TRINITY_THEME=bloomberg
VITE_COORDINATION_MODE=true

# Authentication & Security
VITE_AUTH_PROVIDER=supabase
VITE_SUPABASE_URL=https://[project-ref].supabase.co
VITE_SUPABASE_ANON_KEY=[your-supabase-anon-key]

# Payment Integration
VITE_STRIPE_PUBLISHABLE_KEY=[your-stripe-publishable-key]

# Monitoring & Analytics
VITE_SENTRY_DSN=[your-sentry-frontend-dsn]
VITE_GOOGLE_ANALYTICS_ID=[your-ga4-tracking-id]

# Performance Settings
VITE_DEBUG_MODE=false
VITE_SOUND_ENABLED=true
VITE_AUTO_CONNECT=true
VITE_REFRESH_RATE=1000
VITE_LATENCY_THRESHOLD=200
VITE_MAX_MESSAGE_HISTORY=1000

# Feature Flags
VITE_ORACLE_ENABLED=true
VITE_SENTINEL_ENABLED=true
VITE_SAGE_ENABLED=true
VITE_MCP_REGISTRY_ENABLED=true
VITE_WORKFLOW_BUILDER_ENABLED=true
VITE_REAL_TIME_UPDATES=true

# CDN & Assets
VITE_CDN_URL=https://cdn.openconductor.ai
VITE_ASSETS_VERSION=1.0.0

# WebRTC Configuration (for real-time features)
VITE_WEBRTC_ENABLED=false
VITE_TURN_SERVER_URL=
VITE_TURN_SERVER_USERNAME=
VITE_TURN_SERVER_PASSWORD=