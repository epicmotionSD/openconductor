{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "functions": {
    "app/api/**/*.ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://api.openconductor.ai/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.openconductor.ai"
  }
}