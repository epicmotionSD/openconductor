{
  "name": "@openconductor/core",
  "version": "0.1.0",
  "description": "OpenConductor - Open-source AI agent orchestration platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": {
    "name": "OpenConductor Team",
    "email": "hello@openconductor.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openconductor/core.git"
  },
  "keywords": [
    "ai",
    "agents",
    "orchestration",
    "automation",
    "typescript",
    "open-source"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc && tsc-alias && npm run build:frontend",
    "build:watch": "tsc -w",
    "build:frontend": "cd frontend && npm run build",
    "dev": "tsx watch src/server.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:frontend\"",
    "start": "node dist/server.js",
    "start:frontend": "cd frontend && npm run preview",
    "install:frontend": "cd frontend && npm install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:frontend": "cd frontend && npm run lint && npm run type-check",
    "examples": "tsx examples/run-examples.ts",
    "examples:trinity": "tsx examples/trinity-ai-demo.ts",
    "examples:oracle": "tsx examples/oracle-examples.ts",
    "examples:sentinel": "tsx examples/sentinel-examples.ts",
    "examples:sage": "tsx examples/sage-examples.ts",
    "test:day0": "tsx src/mcp/test-day0-day1-experience.ts",
    "dev:day0": "tsx watch src/mcp/day0-day1-dev-server.ts",
    "demo:day0": "tsx src/mcp/simple-day0-demo.ts",
    "demo:complete": "tsx src/mcp/enhanced-day0-demo-with-aiops.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rimraf dist && cd frontend && npm run clean",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@types/uuid": "^9.0.7",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "bull": "^4.12.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "prom-client": "^15.1.3",
    "redis": "^4.6.12",
    "semver": "^7.7.2",
    "stripe": "^18.5.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^2.0.16",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
